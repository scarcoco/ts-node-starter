{
  "name": "ts-node",
  "version": "1.0.0",
  "description": "ts-node demo",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "compile-run": "tsc && node dist/index.js",
    "watch": "nodemon --watch 'src/**/*' -e ts,tsx,js,jsx --exec 'ts-node' ./src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ./"
  },
  "author": "scarcoco",
  "devDependencies": {
    "@types/node": "^12.11.5",
    "@types/request": "^2.48.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "chalk": "^3.0.0"
  }
}
